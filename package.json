{
  "name": "@nordnetab/grid",
  "version": "0.0.0",
  "description": "Grid system",
  "main": "dist",
  "scripts": {
    "start": "run-p watch docs:start",
    "docs:start": "onpm docs start",
    "docs:build": "cd docs && npm run build && cd ..",
    "watch": "npm run babel -- --watch",
    "test": "ava",
    "tdd": "ava --watch",
    "lint": "eslint .",
    "clean": "rimraf dist",
    "babel": "babel src -d dist --copy-files --ignore test.js,.md",
    "build": "run-s clean babel",
    "prepublish": "npm run build",
    "push": "git push --follow-tags",
    "postpublish": "npm-run-all clean push"
  },
  "keywords": [
    "react",
    "design",
    "ui",
    "ux",
    "grid",
    "column"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "buildbranch": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-pedant": "^0.8.1",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-require-path-exists": "^1.1.5",
    "npm-run-all": "^4.0.2",
    "othernpm": "^1.1.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.6",
    "react": "^15.5.3",
    "react-jss": "^6.0.0"
  }
}
